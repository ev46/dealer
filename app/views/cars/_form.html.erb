<%= simple_form_for @car, :html => { :multipart => true } do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :two_door %>
    <%= f.input :year, collection: (Time.now.year - 10)..(Time.now.year) %>
    <%= f.input :make %>
    <%= f.input :model %>
    <%= f.input :vin %>
    <%= f.input :price %>
    <%= f.input :warranty %>
    <%= file_field_tag('car_attached_assets_avatar', multiple: true, name: "car[attached_assets_attributes][][avatar]") %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
